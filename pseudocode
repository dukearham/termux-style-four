VERSION 1.0.0 TSTYLE4

G: GROUPING AND NOT PRINT AFTER THE G: BUT A VALUE YES PRINT
V: VALUE
P: PAGE
I: INTERACTIVE USER INPUT
C: CONDITION

--------INTERACTIVE--------

START
P:MAIN {
	PRINT {
		PROGRAM TITLE
		DESCRIPTION AND CREDITS
		
		G:INFORMATION {
			CURRENT THEME CV:[NAME OR UNKNOWN OR DEFAULT]
			CURRENT FONT CV:[NAME OR UNKNOWN OR DEFAULT]
		}
		
		ENTER CHOICE {
			[1] THEME
			[2] FONT
			[3] EXTRAS
			[4] HELP
			[5] RESET
			[6] CLEAN
			[0] QUIT
			}
		
		I:INPUT CHOICE
	}
}

AFTER CHOICE: "THEME" "FONT" "EXTRAS" "HELP" "RESET" "CLEAN"

P:THEME {
	PRINT {
		ENTER CHOICE {
			[1] GOGH EXTENDED V:AMOUNT - RECOMMENDED 
			[2] TERMUX STYLE V:AMOUNT
			[3] GOGH DEFAULT V:AMOUNT
			[4] LOCAL / IMPORT THEME
			[0] BACK
		}
		
		I:INPUT CHOICE
		
	}
	
	AFTER CHOICE: 0 - 4
	
	IF CHOICE 0 {
		BACK TO P:MAIN
	} ELIF CHOICE 1 {
		PRINT {
			TITLE GOGH EXTENDED THEME
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
		}
	} ELIF CHOICE 2 {
		PRINT {
			TITLE TERMUX STYLE
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
	} ELIF CHOICE 3 {
		PRINT {
			TITLE GOGH DEFAULT
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
		} ELIF CHOICE 4 {
			I:INPUT PATH FOR THEME
			RELOAD
	} END IF
}

P:FONT {
	PRINT {
		ENTER CHOICE {
			[1] NERD FONT V:AMOUNT
			[2] LOCAL / IMPORT FONT
			[0] BACK
		}
		
		I:INPUT CHOICE
		
	}
	
	AFTER CHOICE: 0 - 2
	
	IF CHOICE 0 {
		BACK TO P:MAIN
	} ELIF CHOICE 1 {
		PRINT {
			TITLE NERD FONT
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME FONT
			[0] BACK P:THEME
		}
	} ELIF CHOICE 2 {
		I:INPUT PATH FOR FONT
		RELOAD
	} END IF
}

