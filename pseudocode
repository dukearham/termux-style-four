G: GROUPING AND NOT PRINT AFTER THE G: BUT A VALUE YES PRINT
V: VALUE
P: PAGE
I: INTERACTIVE USER INPUT
C: CONDITION

--------INTERACTIVE--------

START
P:MAIN {
	PRINT {
		PROGRAM TITLE
		DESCRIPTION AND CREDITS
		
		G:INFORMATION {
			CURRENT THEME CV:[NAME OR UNKNOWN OR DEFAULT]
			CURRENT FONT CV:[NAME OR UNKNOWN OR DEFAULT]
		}
		
		ENTER CHOICE {
			[1] THEME
			[2] FONT
			[3] SHELL
			[3] HELP
			[4] RESET
			[5] WIPE DATA
			[0] QUIT
			}
		
		I:INPUT CHOICE
	}
}

AFTER CHOICE: "THEME" "FONT" "HELP" "RESET" "WIPE DATA"

P:THEME {
	PRINT {
		ENTER CHOICE {
			[1] LOCAL THEME OR SELF IMPORT 
			[2] GOGH THEME V:AMOUNT 
			[3] TERMUX STYLE V:AMOUNT 
			[4] TSTYLE4 V:AMOUNT 
			[0] BACK
		}
		
		I:INPUT CHOICE
		
	}
	
	AFTER CHOICE: 0 - 4
	
	IF CHOICE 0 {
		BACK TO P:MAIN
	} ELIF CHOICE 1 {
		I:INPUT PATH FOR THEME
		RELOAD
	} ELIF CHOICE 2 {
		PRINT {
			TITLE GOGH THEME
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
		}
	} ELIF CHOICE 3 {
		PRINT {
			TITLE TERMUX STYLE
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
		} ELIF CHOICE 4 {
		PRINT {
			TITLE TSTYLE4
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME THEME
			[0] BACK P:THEME
		} END IF
}

P:FONT {
	PRINT {
		ENTER CHOICE {
			[1] LOCAL FONT OR SELF IMPORT 
			[2] NERD FONT V:AMOUNT 
			[0] BACK
		}
		
		I:INPUT CHOICE
		
	}
	
	AFTER CHOICE: 0 - 2
	
	IF CHOICE 0 {
		BACK TO P:MAIN
	} ELIF CHOICE 1 {
		I:INPUT PATH FOR FONT
		RELOAD
	} ELIF CHOICE 2 {
		PRINT {
			TITLE NERD FONT
			DESCRIPTION AND INFORMATION
			[1...AMOUNT] NAME FONT
			[0] BACK P:THEME
		} EMD IF
	}
}

